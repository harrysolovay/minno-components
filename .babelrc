{
  "presets" : [ "babel-preset-expo" ],
  "env" : {
    "development" : {
      "plugins" : [
      	"transform-react-jsx-source",
        [ "module-resolver", {
          "root" : [ "./" ],
          "alias" : {
            'navigation' : './navigation',
            'screens' : './screens',
            'components' : './components',
            'constants' : './constants',
            'stores' : './stores',
            'assets' : './assets'
          }
        }],
      	"transform-decorators-legacy"
      ]
    }
  }
}
